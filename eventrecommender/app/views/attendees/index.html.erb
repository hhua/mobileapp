<h1>Listing attendees</h1>

<table>
  <tr>
    <th>User</th>
    <th>Event</th>
    <th>Saved</th>
    <th>Attending</th>
    <th>Rating</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @attendees.each do |attendee| %>
  <tr>
    <td><%= attendee.user_id %></td>
    <td><%= attendee.event_id %></td>
    <td><%= attendee.saved %></td>
    <td><%= attendee.attending %></td>
    <td><%= attendee.rating %></td>
    <td><%= link_to 'Show', attendee %></td>
    <td><%= link_to 'Edit', edit_attendee_path(attendee) %></td>
    <td><%= link_to 'Destroy', attendee, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Attendee', new_attendee_path %>
